{
    "name": "bnbwebexpertise/laravel-google-cloud-print",
    "description": "Google Cloud Print Service Provider for Laravel 8",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Jérémy Gaulin",
            "email": "jeremy@bnb.re"
        }
    ],
    "autoload": {
        "psr-4": {
            "Bnb\\GoogleCloudPrint\\": "src/"
        }
    },
    "require": {
        "php": "^7.3",
        "illuminate/config": "^7.0|^8.0",
        "illuminate/session": "^7.0|^8.0",
        "illuminate/support": "^7.0|^8.0",
        "google/apiclient": "^2.8"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5|^9.0",
        "mockery/mockery": "^1.0"
    },
    "scripts": {
        "test": "vendor/bin/phpunit --color=always",
        "check": [
            "phpcs --report-width=200 --report-summary  --report-full src/ tests/ --standard=PSR2 -n",
            "phpmd src/,tests/ text ./phpmd.xml.dist"
        ],
        "fix": [
            "phpcs --standard=PSR2 --colors --report-summary --report-full src/ tests/"
        ]
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Bnb\\GoogleCloudPrint\\LaravelServiceProvider"
            ],
            "aliases": {
                "GoogleCloudPrint": "Bnb\\GoogleCloudPrint\\Facades\\GoogleCloudPrint"
            }
        }
    }
}
